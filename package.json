{
  "name": "codeflow-extension",
  "displayName": "codeflow-extension",
  "description": "codeflow-extension",
  "version": "1.23.1",
  "publisher": "heygarrison",
  "galleryBanner": {
    "color": "#e3f4ff",
    "theme": "light"
  },
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/heygarrison/codeflow-extension.git"
  },
  "homepage": "https://github.com/heygarrison/codeflow-extension",
  "bugs": {
    "url": "https://github.com/heygarrison/codeflow-extension/issues"
  },
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other",
    "Keymaps"
  ],
  "extensionKind": [
    "ui"
  ],
  "sideEffects": false,
	"activationEvents": [
		"onCommand:fs/openJS",
		"onCommand:fs/sumSizes",
		"onCommand:fs/readWriteFile"
	],
	"contributes": {
		"commands": [
			{
				"command": "fs/openJS",
				"title": "Open JavaScript-file for TypeScript-file...",
				"category": "FS"
			},
			{
				"command": "fs/sumSizes",
				"title": "Show folder information for current file",
				"category": "FS"
			},
			{
				"command": "fs/readWriteFile",
				"title": "Read and Write file",
				"category": "FS"
			}
		]
	},
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "browser": "./dist/index.js",
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": true
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "pnpm run build --watch",
    "symlink": "ln -s ~/HeyGarrison/codeflow-extension  ~/.stackblitz/extensions/codeflow-extension",
    "lint": "eslint .",
    "vscode:prepublish": "pnpm run build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@types/node": "^18.6.4",
    "@types/vscode": "^1.69.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "pnpm": "^7.8.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "vsce": "^2.10.0"
  }
}

